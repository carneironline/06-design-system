{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [
                "dist/**",
                "storybook-static/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "dev": {
            "cache": false
        }
    }
}
